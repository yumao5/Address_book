<!DOCTYPE html>
<html>
  <head>
    <title>Address Book | <%= @page_title || "Admin" %></title>
    <%= stylesheet_link_tag('application', :media => 'all') %>
  </head>
  <body>
    <div class="container">

      <div class="row">
        <div class="row text-center">
          <h3>Address Book List</h3>
        </div>

        <div class="row ">
          <% if !flash[:notice].blank? %>
            <div class="info text-center">
              <%= flash[:notice] %>
            </div>
          <% end %>
          <div id="content" >
            <%= yield %>
          </div>
        </div>

      </div>

    </div>
  </body>
</html>
